{
    "name": "rest-sveltekit",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "generate": "zenstack generate",
        "generate:push": "yarn generate && yarn db:push",
        "debug": "vavite-loader vite dev",
        "dev": "vite dev --port 4069",
        "build": "vite build",
        "vercel-build": "npm run generate && npm run build && npm run db:deploy",
        "preview": "vite preview",
        "db:up": "docker compose up -d",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:deploy": "prisma migrate deploy",
        "db:reset": "prisma migrate reset",
        "db:browse": "prisma studio",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "package-clean": "npm rm zenstack @zenstackhq/runtime @zenstackhq/server @zenstackhq/tanstack-query",
        "up": "npm run package-clean && npm install -D --save-exact zenstack@latest && npm install --save-exact @zenstackhq/runtime@latest @zenstackhq/server@latest @zenstackhq/tanstack-query@latest",
        "up-preview": "npm run package-clean && npm install --registry https://preview.registry.zenstack.dev -D --save-exact zenstack@latest && npm install --registry https://preview.registry.zenstack.dev --save-exact @zenstackhq/runtime@latest @zenstackhq/server@latest @zenstackhq/tanstack-query@latest"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^2.1.0",
        "@sveltejs/kit": "^1.22.4",
        "@sveltejs/package": "^2.2.0",
        "@tailwindcss/line-clamp": "^0.4.4",
        "@types/bcryptjs": "^2.4.2",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/lodash": "^4.14.199",
        "@types/node-schedule": "^2.1.1",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@zenstackhq/openapi": "^1.0.2",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "postcss": "^8.4.23",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.8.1",
        "prisma": "^5.1.1",
        "run-s": "^0.0.0",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.6",
        "svelte": "^4.1.2",
        "svelte-check": "^3.4.6",
        "tailwindcss": "^3.3.2",
        "ts-node": "10.9.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^4.4.4",
        "zenstack": "1.0.2"
    },
    "type": "module",
    "dependencies": {
        "@auth/core": "^0.16.1",
        "@auth/prisma-adapter": "^1.0.3",
        "@auth/sveltekit": "^0.3.9",
        "@prisma/client": "^5.4.1",
        "@steeze-ui/heroicons": "^2.2.3",
        "@steeze-ui/svelte-icon": "^1.5.0",
        "@tanstack/svelte-query": "^4.32.6",
        "@tanstack/svelte-table": "^8.10.6",
        "@zenstackhq/runtime": "1.0.2",
        "@zenstackhq/server": "1.0.2",
        "@zenstackhq/tanstack-query": "1.0.2",
        "bcryptjs": "^2.4.3",
        "daisyui": "^3.9.2",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "nanoid": "^4.0.2",
        "node-schedule": "^2.1.1",
        "pokemon-tcg-sdk-typescript": "^1.3.4",
        "superjson": "^1.12.3",
        "svelte-media-queries": "^1.6.2",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-svelte": "^0.0.13",
        "validator": "^13.11.0",
        "zod": "3.21.1",
        "zod-validation-error": "^1.3.0"
    }
}
